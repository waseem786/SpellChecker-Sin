<IfModule mod_rewrite.c>

    RewriteEngine On

	RewriteCond %{REQUEST_FILENAME} !-d
        RewriteCond %{REQUEST_FILENAME} !-s
        RewriteRule ^([a-zA-Z0-9-]+)/studentapi/method/ student.php?x=$1 [QSA,NC,L]
        RewriteRule ^([a-zA-Z0-9-]+)/staffapi/method/ staff.php?x=$1 [QSA,NC,L]
        RewriteRule ^([a-zA-Z0-9-]+)/courseapi/method/ courses.php?x=$1 [QSA,NC,L]
        

     
</IfModule>